{
  "sourceType": "module",
  "name": "colyseus-examples",
  "version": "1.0.0",
  "description": "Usage Examples of Colyseus Game Server",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch '**/*.ts' index.ts",
    "inspect": "node --inspect --require ts-node/register index.ts",
    "bundle-colyseus-client": "browserify ./node_modules/colyseus.js/lib/index.js -s Colyseus -o static/colyseus.js",
    "build": "npm run bundle-colyseus-client",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "10.19.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@colyseus/monitor": "^0.1.7",
    "colyseus": "^0.10.9",
    "colyseus.js": "^0.10.2",
    "express": "^4.14.0",
    "nodemon": "^1.19.4",
    "serve-index": "^1.8.0",
    "superagent": "^3.8.1"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  }
}
